{
  "accuracy_in_millis": 100,
  "scale": {
    "x_in_cm": 1,
    "y_in_cm": 1,
    "z_in_cm": 1,
    "speed_in_cm_per_sec": 1
  },
  "controllers": {
    "my-controller-1": {
      "type": "MicroPython",
      "type-micropython-config": {
        "ssid": "your-wifi-ssid",
        "passphrase": "your-wifi-passphrase",
        "com_descriptor": "/dev/cu.SLAB_USBtoUART"
      }
    }
  },
  "sequence": {
    "0": [
      {
        "command": "command",
        "params": [],
        "controllers": ["my-controller-1"]
      }
    ],
    "5000": [
      {
        "command": "set_speed",
        "params": ["100"],
        "controllers": ["my-controller-1"]
      }
    ],
    "6000": [
      {
        "command": "takeoff",
        "params": [],
        "controllers": ["my-controller-1"]
      }
    ],
    "10000": [
      {
        "command": "down",
        "params": ["30"],
        "controllers": ["my-controller-1"]
      }
    ],
    "15000": [
      {
        "command": "flip",
        "params": ["f"],
        "controllers": ["my-controller-1"]
      }
    ],
    "20000": [
      {
        "command": "flip",
        "params": ["b"],
        "controllers": ["my-controller-1"]
      }
    ],
    "25000": [
      {
        "command": "land",
        "params": [],
        "controllers": ["my-controller-1"]
      }
    ]
  }
}
