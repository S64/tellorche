<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jp.s64.tellorche.gui.scene.first.FirstSceneController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tellorche GUI">
         <font>
            <Font name="System Bold" size="40.0" />
         </font></Text>
      <Button fx:id="sequenceButton" mnemonicParsing="false" onAction="#onClickExecuteSequence" text="シーケンス実行" />
      <Button mnemonicParsing="false" onAction="#onClickSerialPortsButton" text="シリアルポート一覧" />
   </children>
</VBox>
